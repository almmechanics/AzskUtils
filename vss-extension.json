{
  "manifestVersion": 1,
  "id": "AzskNUnit",
  "version": "1.4.12",
  "name": "Utilities to support AzSK files",
  "description": "Utilities to support AzSK files",
  "publisher": "ALMMechanics",
  "public": false,
  "links": {
    "repository": {
      "type": "git",
      "uri": "https://github.com/almmechanics/AzskUtils"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "scopes": [
    "vso.release"
  ],
  "tags": [
    "release",
    "build",
    "archive",
    "azsk",
    "arm",
    "unit",
    "CI",
    "CD"
  ],
  "screenshots": [],
  "branding": {
    "color": "rgb(34, 34, 34)",
    "theme": "dark"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "files": [
    {
      "path": "tasks/Azsk-NUnit"
    },
    {
      "path": "tasks/Azsk-svt-NUnit"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "azsk-nunit-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/Azsk-NUnit"
      }
    },
    {
      "id": "azsk-svt-nunit-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/Azsk-svt-NUnit"
      }
    }
  ]
}